                    }
                },
                {
                    "on_damage": {
                        "filters": {
                            "all_of": [
                                {
                                    "test": "has_damage",
                                    "operator": "==",
                                    "value": "projectile"
                                },
                                {
                                    "test": "is_family",
                                    "operator": "==",
                                    "subject": "other",
                                    "value": "player"
                                },
                                {
                                    "test": "has_damage",
                                    "operator": "==",
                                    "subject": "self",
                                    "value": "fatal"
                                }
                            ]
                        },
                        "event": "astro:player_death"
                    }
                },
                {
                    "on_damage": {
                        "filters": {
                            "all_of": [
                                {
                                    "test": "has_damage",
                                    "operator": "==",
                                    "value": "magic"
                                },
                                {
                                    "test": "is_family",
                                    "operator": "==",
                                    "subject": "other",
                                    "value": "player"
                                },
                                {
                                    "test": "has_damage",
                                    "operator": "==",
                                    "subject": "self",
                                    "value": "fatal"
                                }
                            ]
                        },
                        "event": "astro:player_death"
                    }
                }
            ]
        }
    },
    "events": {
        "astro:player_death": {
            "run_command": {
                "target": "self",
