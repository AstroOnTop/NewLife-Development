                    "max_rider_count": 2,
                    "lock_rider_rotation": 0
                }
            ]
        },
        "minecraft:scaffolding_climber": {},
        "minecraft:npc": {},
        "minecraft:damage_sensor": {
            "triggers": [
                {
                    "on_damage": {
                        "filters": {
                            "any_of": [
                                {
                                    "all_of": [
                                        {
                                            "test": "has_tag",
                                            "value": "32kable",
                                            "subject": "self"
                                        },
                                        {
                                            "test": "has_damage",
                                            "value": "fatal"
                                        }
                                    ]
                                }
                            ]
                        }
                    },
                    "deals_damage": false
                },
                {
                    "on_damage": {
                        "filters": {
                            "all_of": [
                                {
                                    "test": "has_damage",
                                    "operator": "==",
                                    "value": "attack"
                                },
                                {
                                    "test": "is_family",
                                    "operator": "==",
                                    "subject": "other",
                                    "value": "player"
                                },
                                {
                                    "test": "has_damage",
                                    "operator": "==",
                                    "subject": "self",
                                    "value": "fatal"
                                }
                            ]
                        },
